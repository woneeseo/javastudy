package com.naver;

public class MainEx {
	public static void main(String[] args) {
	
	 char c = 'A'+32;
	 System.out.println(c);
	 boolean b = false;
	 System.out.println(b);
	 long longVa = 3000000000L;
	 System.out.println(longVa);
	 
	 Dog dog = new Dog();
	 
	 System.out.println("::::::::::");
	 
	 int i = 56;
	 System.out.println(i);
	 
	}

}

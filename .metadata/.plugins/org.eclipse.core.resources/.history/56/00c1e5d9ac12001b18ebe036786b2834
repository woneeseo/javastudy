package kr.co.ezen;

public class Method {
	
	public Method() {
		
	}
	
	public void hello() {
		System.out.println("hello");
	}
	
	public void score(String name, String text, int score) {
		System.out.println(name + "의 " + text + "점수는 " + score + "점 이다.");
	}
	
	public void printDan(int a) {
		System.out.println(a+"x1="+a*1);
		System.out.println(a+"x1="+a*2);
		System.out.println(a+"x1="+a*3);
		System.out.println(a+"x1="+a*4);
		System.out.println(a+"x1="+a*5);
		System.out.println(a+"x1="+a*6);
		System.out.println(a+"x1="+a*7);
		System.out.println(a+"x1="+a*8);
		System.out.println(a+"x1="+a*9);
	}
	
	
	

}

package com.naver;

public class cat {

	String name = "happy";
	// member 변수 (instance 변수)
	String master;
	int age;
	

	public cat() {
		
		System.out.println(name);
		int b = 33;
		System.out.println(b);
		me1();
		
	}
	
	public void me1(int b) {
		
		System.out.println(name);
		System.out.println(b);
		
	}
	

}
